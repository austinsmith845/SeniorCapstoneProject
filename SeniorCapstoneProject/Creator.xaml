<Window x:Class="SeniorCapstoneProject.Creator"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeniorCapstoneProject"
        mc:Ignorable="d"
        Title="Creator" Height="800" Width="1200" ResizeMode="NoResize"  KeyDown="Window_KeyDown" >
    <Grid x:Name="Grid" HorizontalAlignment="Left" Height="790" VerticalAlignment="Top" Width="1200" Margin="0,0,-15,-19.6" ClipToBounds="True" SnapsToDevicePixels="True">
        <Menu HorizontalAlignment="Right" Height="29" VerticalAlignment="Top" Width="1200">
            <MenuItem x:Name="Insert" Header="  Insert">
                <MenuItem x:Name="Recliner" Header="Recliner" Tag="Recliner" Click="MenuClick"/>
            </MenuItem>
            <MenuItem x:Name="Help" Header="Help">
                <MenuItem x:Name="Controls" Header="Controls" Tag="Controls" Click="MenuClick"/>
            </MenuItem>
        </Menu>
    </Grid>






</Window>
